# Environment Configuration for Sustainable Urban Studio Backend\n\n# Server Configuration\nPORT=3001\nNODE_ENV=development\n\n# Database Configuration\nDATABASE_URL=postgresql://postgres:your_password_here@localhost:5432/urban_studio\n\n# Cache Configuration\nCACHE_TTL=3600000\nCACHE_CHECK_PERIOD=120000\n\n# Rate Limiting Configuration\nRATE_LIMIT_WINDOW_MS=900000\nRATE_LIMIT_MAX_REQUESTS=100\nDATA_DOWNLOAD_LIMIT_MAX=10\nDATA_DOWNLOAD_LIMIT_WINDOW_MS=3600000\n\n# External API Rate Limits (requests per hour)\nCENSUS_API_RATE_LIMIT=500\nNATURAL_EARTH_API_RATE_LIMIT=100\nOSM_API_RATE_LIMIT=60\nMUNICIPAL_API_RATE_LIMIT=200\nMICROSOFT_API_RATE_LIMIT=1000\n\n# Download Configuration\nMAX_DOWNLOAD_SIZE=5368709120\nDOWNLOAD_TIMEOUT=1800000\nCONCURRENT_DOWNLOADS=3\n\n# File Processing Configuration\nMAX_FEATURES_PER_DATASET=1000000\nTEMP_DIR=./downloads\nCLEANUP_INTERVAL=86400000\n\n# Security Configuration\nENABLE_HELMET=true\nENABLE_COMPRESSION=true\nCORS_ORIGIN=http://localhost:8080\n\n# Logging Configuration\nLOG_LEVEL=info\nENABLE_ACCESS_LOGS=true\n\n# External API URLs (typically shouldn't need to change)\nNATURAL_EARTH_BASE_URL=https://naturalearth.s3.amazonaws.com\nGEOFABRIK_BASE_URL=https://download.geofabrik.de\nCENSUS_TIGER_BASE_URL=https://www2.census.gov/geo/tiger\nCENSUS_API_BASE_URL=https://api.census.gov/data\nMICROSOFT_BUILDINGS_BASE_URL=https://minedbuildings.blob.core.windows.net/global-buildings/2023-01-15\n\n# NYC Open Data\nNYC_OPEN_DATA_BASE_URL=https://data.cityofnewyork.us/api/geospatial\n\n# California Open Data  \nCA_OPEN_DATA_BASE_URL=https://data.ca.gov/api/3/action\n\n# San Francisco Open Data\nSF_OPEN_DATA_BASE_URL=https://data.sfgov.org/api/geospatial\n\n# Los Angeles Open Data\nLA_OPEN_DATA_BASE_URL=https://data.lacity.org/api/geospatial"